-- Define GUI
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local CloseButton = Instance.new("TextButton")
local Title = Instance.new("TextLabel")
local PackCookButton = Instance.new("TextButton")
local BarrelButton = Instance.new("TextButton")
local BagsButton = Instance.new("TextButton")

-- Configure GUI components
ScreenGui.Name = "TeleportHub"
ScreenGui.Parent = game.CoreGui

Frame.Name = "MainFrame"
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Frame.Size = UDim2.new(0, 300, 0, 250)
Frame.Position = UDim2.new(0.5, -150, 0.5, -125)
Frame.Active = true
Frame.Draggable = true

CloseButton.Name = "CloseButton"
CloseButton.Parent = Frame
CloseButton.Text = "X"
CloseButton.Size = UDim2.new(0, 30, 0, 30)
CloseButton.Position = UDim2.new(1, -35, 0, 5)
CloseButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
CloseButton.Font = Enum.Font.SourceSansBold
CloseButton.TextSize = 16
CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

Title.Name = "Title"
Title.Parent = Frame
Title.Text = "Teleport Hub"
Title.Size = UDim2.new(1, -40, 0, 30)
Title.Position = UDim2.new(0, 5, 0, 5)
Title.BackgroundTransparency = 1
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.Font = Enum.Font.SourceSansBold
Title.TextSize = 20
Title.TextXAlignment = Enum.TextXAlignment.Left

PackCookButton.Name = "PackCookButton"
PackCookButton.Parent = Frame
PackCookButton.Text = "Pack/Cook"
PackCookButton.Size = UDim2.new(0, 200, 0, 50)
PackCookButton.Position = UDim2.new(0.5, -100, 0.5, -100)
PackCookButton.BackgroundColor3 = Color3.fromRGB(50, 150, 250)
PackCookButton.TextColor3 = Color3.fromRGB(255, 255, 255)
PackCookButton.Font = Enum.Font.SourceSansBold
PackCookButton.TextSize = 18

BarrelButton.Name = "BarrelButton"
BarrelButton.Parent = Frame
BarrelButton.Text = "Barrel"
BarrelButton.Size = UDim2.new(0, 200, 0, 50)
BarrelButton.Position = UDim2.new(0.5, -100, 0.5, -40)
BarrelButton.BackgroundColor3 = Color3.fromRGB(50, 150, 250)
BarrelButton.TextColor3 = Color3.fromRGB(255, 255, 255)
BarrelButton.Font = Enum.Font.SourceSansBold
BarrelButton.TextSize = 18

BagsButton.Name = "BagsButton"
BagsButton.Parent = Frame
BagsButton.Text = "Bags"
BagsButton.Size = UDim2.new(0, 200, 0, 50)
BagsButton.Position = UDim2.new(0.5, -100, 0.5, 20)
BagsButton.BackgroundColor3 = Color3.fromRGB(50, 150, 250)
BagsButton.TextColor3 = Color3.fromRGB(255, 255, 255)
BagsButton.Font = Enum.Font.SourceSansBold
BagsButton.TextSize = 18

-- Teleport functions
local function teleportActions()
    local player = game.Players.LocalPlayer
    if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        -- Teleport player
        player.Character.HumanoidRootPart.CFrame = CFrame.new(10279, 133, 1024)
        wait(1)
        player.Character.HumanoidRootPart.CFrame = CFrame.new(10279, 133, 1026)

        -- Move parts simultaneously
        for i = 1, 2 do
            workspace.Interactions["1809"]["1809"].CFrame = CFrame.new(10279, 133, 1026)
            workspace.Interactions["1806"]["1806"].CFrame = CFrame.new(10268, 133, 1026)
            wait(2)
        end
    else
        warn("Player or HumanoidRootPart not found")
    end
end

local function barrelActions()
    local player = game.Players.LocalPlayer
    if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        -- Teleport player
        player.Character.HumanoidRootPart.CFrame = CFrame.new(6261, 227, 2903)
        wait(1)
        player.Character.HumanoidRootPart.CFrame = CFrame.new(6261, 227, 2903)

        -- Move part multiple times
        for i = 1, 2 do
            workspace.Interactions["253"]["253"].CFrame = CFrame.new(6261, 227, 2905)
            wait(2)
        end
    else
        warn("Player or HumanoidRootPart not found")
    end
end

local function bagsActions()
    local player = game.Players.LocalPlayer
    if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        -- Teleport player
        player.Character.HumanoidRootPart.CFrame = CFrame.new(6396, 214, 2926)
        wait(1)
        player.Character.HumanoidRootPart.CFrame = CFrame.new(6396, 214, 2926)

        -- Move part multiple times
        for i = 1, 2 do
            workspace.Interactions["270"]["270"].CFrame = CFrame.new(6396, 214, 2926)
            wait(2)
        end
    else
        warn("Player or HumanoidRootPart not found")
    end
end

-- Button actions
PackCookButton.MouseButton1Click:Connect(teleportActions)
BarrelButton.MouseButton1Click:Connect(barrelActions)
BagsButton.MouseButton1Click:Connect(bagsActions)
