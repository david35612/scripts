-- Button Functionality
TeleportsButton.MouseButton1Click:Connect(function()
    -- Teleport to the LaunderMat
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(-2006, 210, -944)
    end
end)

DealershipButton.MouseButton1Click:Connect(function()
    -- Teleport to the Dealership Main
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(-1757, 209, -1222)
    end
end)

LockPickButton.MouseButton1Click:Connect(function()
    -- Teleport to the LockPick2 location
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(-2416, 233, -1170)
    end
end)

TecButton.MouseButton1Click:Connect(function()
    -- Teleport to the Tec location
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(-1475, 228, -667)
    end
end)

-- Bank button functionality
BankButton.MouseButton1Click:Connect(function()
    -- Teleport to the Bank position: 1260, 210, -129
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(1260, 210, -129)
    end
end)

-- Mersea button functionality
MerseaButton.MouseButton1Click:Connect(function()
    -- Teleport to the Mersea position: 6399, 226, 3154
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(6399, 226, 3154)
    end
end)

-- Refresh button functionality
RefreshButton.MouseButton1Click:Connect(function()
    -- Teleport to the Refresh position: -347, 210, 2704
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(-347, 210, 2704)
    end
end)

-- Close Button Functionality
CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy() -- Close the GUI
end)

-- Draggable GUI (Optional)
local UIS = game:GetService("UserInputService")
local dragging, dragInput, dragStart, startPos

MainFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
    end
end)

MainFrame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInput = input
    end
end)

UIS.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        local delta = input.Position - dragStart
        MainFrame.Position = UDim2.new(
            startPos.X.Scale,
            startPos.X.Offset + delta.X,
            startPos.Y.Scale,
            startPos.Y.Offset + delta.Y
        )
    end
end)

UIS.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)
