-- Create GUI Hub
local ScreenGui = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local TeleportsButton = Instance.new("TextButton")
local DealershipButton = Instance.new("TextButton")
local LockPickButton = Instance.new("TextButton")  -- LockPick2 button
local TecButton = Instance.new("TextButton")  -- New button for Tec
local CloseButton = Instance.new("TextButton")

-- Parent the GUI
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- Properties
ScreenGui.Name = "TeleportHub"
ScreenGui.ResetOnSpawn = false

MainFrame.Name = "MainFrame"
MainFrame.Parent = ScreenGui
MainFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
MainFrame.Position = UDim2.new(0.5, -150, 0.5, -100)
MainFrame.Size = UDim2.new(0, 300, 0, 350)  -- Increased height to fit the new button
MainFrame.BorderSizePixel = 0

Title.Name = "Title"
Title.Parent = MainFrame
Title.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
Title.Size = UDim2.new(1, 0, 0, 50)
Title.Font = Enum.Font.SourceSansBold
Title.Text = "Therapy Hub"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.TextSize = 24

TeleportsButton.Name = "TeleportsButton"
TeleportsButton.Parent = MainFrame
TeleportsButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
TeleportsButton.Position = UDim2.new(0.1, 0, 0.2, 0)
TeleportsButton.Size = UDim2.new(0.8, 0, 0.2, 0)
TeleportsButton.Font = Enum.Font.SourceSansBold
TeleportsButton.Text = "LaunderMat"
TeleportsButton.TextColor3 = Color3.fromRGB(255, 255, 255)
TeleportsButton.TextSize = 18

DealershipButton.Name = "DealershipButton"
DealershipButton.Parent = MainFrame
DealershipButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
DealershipButton.Position = UDim2.new(0.1, 0, 0.6, 0)
DealershipButton.Size = UDim2.new(0.8, 0, 0.2, 0)
DealershipButton.Font = Enum.Font.SourceSansBold
DealershipButton.Text = "Dealership Main"
DealershipButton.TextColor3 = Color3.fromRGB(255, 255, 255)
DealershipButton.TextSize = 18

LockPickButton.Name = "LockPickButton"  -- LockPick2 button
LockPickButton.Parent = MainFrame
LockPickButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
LockPickButton.Position = UDim2.new(0.1, 0, 0.8, 0)  -- Adjusted position for the new button
LockPickButton.Size = UDim2.new(0.8, 0, 0.2, 0)
LockPickButton.Font = Enum.Font.SourceSansBold
LockPickButton.Text = "LockPick2"
LockPickButton.TextColor3 = Color3.fromRGB(255, 255, 255)
LockPickButton.TextSize = 18

TecButton.Name = "TecButton"  -- Tec button
TecButton.Parent = MainFrame
TecButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
TecButton.Position = UDim2.new(0.1, 0, 1.0, 0)  -- Adjusted position for the new button
TecButton.Size = UDim2.new(0.8, 0, 0.2, 0)
TecButton.Font = Enum.Font.SourceSansBold
TecButton.Text = "Tec"
TecButton.TextColor3 = Color3.fromRGB(255, 255, 255)
TecButton.TextSize = 18

CloseButton.Name = "CloseButton"
CloseButton.Parent = MainFrame
CloseButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
CloseButton.Position = UDim2.new(0.9, 0, 0, 0)
CloseButton.Size = UDim2.new(0.1, 0, 0.1, 0)
CloseButton.Font = Enum.Font.SourceSansBold
CloseButton.Text = "X"
CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
CloseButton.TextSize = 18

-- Button Functionality
TeleportsButton.MouseButton1Click:Connect(function()
    -- Teleport to the LaunderMat
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(-2006, 210, -944)
    end

    -- Destroy the Door
    local doorPath = workspace:FindFirstChild("Buildings")
        and workspace.Buildings:FindFirstChild("SteinwegCove")
        and workspace.Buildings.SteinwegCove:FindFirstChild("SteinwegCoveModel")
        and workspace.Buildings.SteinwegCove.SteinwegCoveModel:FindFirstChild("SteinwegCoveStoreFillerFive")
        and workspace.Buildings.SteinwegCove.SteinwegCoveModel.SteinwegCoveStoreFillerFive:FindFirstChild("SteinwegCoveStoreFillerFiveModel")
        and workspace.Buildings.SteinwegCove.SteinwegCoveModel.SteinwegCoveStoreFillerFive.SteinwegCoveStoreFillerFiveModel:FindFirstChild("SteinwegCoveStoreFillerFiveModel")
        and workspace.Buildings.SteinwegCove.SteinwegCoveModel.SteinwegCoveStoreFillerFive.SteinwegCoveStoreFillerFiveModel.SteinwegCoveStoreFillerFiveModel:FindFirstChild("Door")

    if doorPath then
        doorPath:Destroy()
    else
        warn("Door not found at the specified path.")
    end

    -- Remove the blocking part
    local blockPath = workspace:FindFirstChild("Functions")
        and workspace.Functions:FindFirstChild("Businesses")
        and workspace.Functions.Businesses:FindFirstChild("City Used Cars")
        and workspace.Functions.Businesses["City Used Cars"]:FindFirstChild("Medium Stock")
        and workspace.Functions.Businesses["City Used Cars"]["Medium Stock"]:FindFirstChild("Part")

    if blockPath then
        blockPath:Destroy()
    else
        warn("Blocking part not found at the specified path.")
    end
end)

DealershipButton.MouseButton1Click:Connect(function()
    -- Teleport to the Dealership Main
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(-1757, 209, -1222)
    end

    -- Destroy the Door for Dealership
    local doorPath = workspace:FindFirstChild("Buildings")
        and workspace.Buildings:FindFirstChild("Bowrey")
        and workspace.Buildings.Bowrey:FindFirstChild("BowreyModel")
        and workspace.Buildings.Bowrey.BowreyModel:FindFirstChild("CityUsedCars")
        and workspace.Buildings.Bowrey.BowreyModel.CityUsedCars:FindFirstChild("CityUsedCarsModel")
        and workspace.Buildings.Bowrey.BowreyModel.CityUsedCars.CityUsedCarsModel:FindFirstChild("CityUsedCarsModel")
        and workspace.Buildings.Bowrey.BowreyModel.CityUsedCars.CityUsedCarsModel.CityUsedCarsModel:FindFirstChild("Model")

    if doorPath then
        doorPath:Destroy()
    else
        warn("Door not found at the specified path.")
    end

    -- Remove the blocking part for Dealership
    local blockPath = workspace:FindFirstChild("Functions")
        and workspace.Functions:FindFirstChild("Businesses")
        and workspace.Functions.Businesses:FindFirstChild("City Used Cars")
        and workspace.Functions.Businesses["City Used Cars"]:FindFirstChild("Medium Stock")
        and workspace.Functions.Businesses["City Used Cars"]["Medium Stock"]:FindFirstChild("Part")

    if blockPath then
        blockPath:Destroy()
    else
        warn("Blocking part not found at the specified path.")
    end
end)

LockPickButton.MouseButton1Click:Connect(function()
    -- Teleport to the LockPick2 location
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(-2416, 233, -1170)
    end
end)

TecButton.MouseButton1Click:Connect(function()
    -- Teleport to the Tec location
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(-1475, 228, -667)
    end
end)

-- Close Button Functionality
CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy() -- Close the GUI
end)

-- Draggable GUI (Optional)
local UIS = game:GetService("UserInputService")
local dragging, dragInput, dragStart, startPos

MainFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
    end
end)

MainFrame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInput = input
    end
end)

UIS.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        local delta = input.Position - dragStart
        MainFrame.Position = UDim2.new(
            startPos.X.Scale,
            startPos.X.Offset + delta.X,
            startPos.Y.Scale,
            startPos.Y.Offset + delta.Y
        )
    end
end)

UIS.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)
