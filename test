-- List of audio IDs (replace with any valid Roblox sound IDs)
local soundIDs = {
    "rbxassetid://1845244245", -- Example sound 1 (Roar)
    "rbxassetid://220153632", -- Example sound 2 (Background music)
    "rbxassetid://1171245002", -- Example sound 3 (Explosion)
    "rbxassetid://224687199", -- Example sound 4 (Ambient sound)
    "rbxassetid://5094703474" -- Example sound 5 (Chime)
}

-- Function to play random sound with enhanced volume
local function playRandomSound()
    -- Create a Sound object
    local sound = Instance.new("Sound")
    sound.SoundId = soundIDs[math.random(1, #soundIDs)] -- Pick a random sound from the list
    sound.Volume = 3000 -- Set volume to 3000% (or 30x normal volume)
    sound.Parent = workspace -- Attach the sound to the workspace so it can be heard
    sound:Play() -- Play the sound

    -- After the sound finishes, remove it to prevent clutter
    sound.Ended:Connect(function()
        sound:Destroy() -- Clean up the sound after it ends
    end)
end

-- Loop the sound playing every 5 seconds with a random sound
while true do
    playRandomSound() -- Play a random sound
    wait(5) -- Wait for 5 seconds before playing the next sound
end
